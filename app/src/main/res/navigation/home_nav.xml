<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/home_nav"
    app:startDestination="@id/homeScreenFragment">
    <fragment
        android:id="@+id/homeScreenFragment"
        android:name="com.example.talks.home.homeScreen.HomeScreenFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_home_screen" >
        <action
            android:id="@+id/action_homeScreenFragment_to_contactScreenFragment2"
            app:destination="@id/contactScreenFragment"
            app:enterAnim="@anim/nav_default_enter_anim" />
        <action
            android:id="@+id/action_homeScreenFragment_to_settingsFragment2"
            app:destination="@id/settingsFragment"
            app:enterAnim="@anim/nav_default_enter_anim" />
    </fragment>
    <fragment
        android:id="@+id/contactScreenFragment"
        android:name="com.example.talks.home.contactScreen.ContactScreenFragment"
        android:label="Contacts"
        tools:layout="@layout/fragment_contact_screen" >
        <action
            android:id="@+id/action_contactScreenFragment_to_homeScreenFragment2"
            app:destination="@id/homeScreenFragment"
            app:enterAnim="@anim/nav_default_enter_anim" />
    </fragment>
    <fragment
        android:id="@+id/settingsFragment"
        android:name="com.example.talks.home.SettingsFragment"
        android:label="SettingsFragment" >
        <action
            android:id="@+id/action_settingsFragment_to_homeScreenFragment2"
            app:destination="@id/homeScreenFragment"
            app:enterAnim="@anim/nav_default_enter_anim" />
    </fragment>
</navigation>